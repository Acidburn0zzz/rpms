diff -Naupr mach-0.4.8.orig/etc/dist.d/rhel-4AS-i386 mach-0.4.8-cfgchange/etc/dist.d/rhel-4AS-i386
--- mach-0.4.8.orig/etc/dist.d/rhel-4AS-i386	1970-01-01 01:00:00.000000000 +0100
+++ mach-0.4.8-cfgchange/etc/dist.d/rhel-4AS-i386	2005-12-19 18:59:59.000000000 +0100
@@ -0,0 +1,34 @@
+# mach dist configuration                                        -*- python -*-
+
+V = '4AS'                     # distro version
+A = 'i386'                    # architecture
+AS = ''                       # 'short' version of arch
+DVA = 'rhel-' + V + '-' + A   # distro-version-architecture
+
+yumsources[DVA] = {
+  'os':               rhelyum + '/el' + V + '/' + A + '/os',
+  'updates':          rhelyum + '/el' + V + '/' + A + '/updates',
+}
+
+# RHEL
+packages[DVA + '-os'] = {
+  'dir':      DVA,
+  'minimal':  'bash glibc redhat-release',
+  'base':     'coreutils findutils openssh-server',
+  'build':    'dev redhat-rpm-config rpm-build make gcc gcc-c++ tar gzip ' +
+              'patch unzip bzip2 diffutils cpio elfutils',
+}
+sourceslist[DVA + '-os'] = {
+  DVA : ('os', )
+}
+config[DVA + '-os'] = {}
+aliases[DVA + '-os'] = ('rh' + V + AS, 'rhel' + V + AS, )
+
+# RHEL with updates
+packages[DVA + '-updates'] = packages[DVA + '-os']
+sourceslist[DVA + '-updates'] = {
+  DVA : ('os', 'updates', )
+}
+config[DVA + '-updates'] = config[DVA + '-os']
+aliases[DVA + '-updates'] = ('rh' + V + 'u' + AS, 'rhel' + V + 'u' + AS, )
+
